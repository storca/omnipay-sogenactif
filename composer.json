{
  "name": "gvalfr35/omnipay-sogenactif",
  "type": "library",
  "description": "Sogenactif Sogenactif(v2) support for Omnipay payment processing library",
  "keywords": [
    "shell",
    "gateway",
    "merchant",
    "omnipay",
    "pay",
    "payment",
    "purchase",
    "Sogenactif",
    "Attendize"
  ],
  "homepage": "https://github.com/storca/omnipay-sogenactif",
  "license": "MIT",
  "authors": [
    {
      "name": "storca",
      "email": "storca@mail.com"
    }
  ],
  "autoload": {
    "psr-4": { "Omnipay\\Sogenactif\\" : "src/" }
  },
    "require": {
        "omnipay/common": "^3"
    },
    "require-dev": {
        "omnipay/tests": "^3",
        "squizlabs/php_codesniffer": "^3"
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "check-style": "phpcs -p --standard=PSR2 src/",
        "fix-style": "phpcbf -p --standard=PSR2 src/"
    }
}
